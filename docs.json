{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "WebRun Documentation",
  "colors": {
    "primary": "#0037B9",
    "light": "#015cbd",
    "dark": "#003d9d"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "getting-started/overview",
              "getting-started/quickstart",
              "getting-started/workflow-quickstart",
              "getting-started/mcp-quickstart"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/sessions",
              "concepts/tasks",
              "concepts/guardrails",
              "concepts/response-model",
              "concepts/pricing"
            ]
          },
          {
            "group": "Usage Guides",
            "pages": [
              "usage-guides/multi-task-workflows",
              "usage-guides/controlling-sessions",
              "usage-guides/manual-interaction",
              "usage-guides/handling-guardrails",
              "usage-guides/video-streaming",
              "usage-guides/structured-output",
              "usage-guides/cost-optimization"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/rest-api",
              "integrations/websocket",
              "integrations/openai-compatible",
              "integrations/mcp-server"
            ]
          },
          {
            "group": "API Reference",
            "pages": [
              "api-reference/endpoints",
              "api-reference/parameters",
              "api-reference/response-formats",
              "api-reference/rate-limits"
            ]
          },
            {
            "group": "Capabilities",
            "pages": [
              "capabilities/supported-interactions"
            ]
          },
          {
            "group": "Troubleshooting",
            "pages": [
              "troubleshooting/errors-reference",
              "troubleshooting/common-issues",
              "troubleshooting/faqs"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [

      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@webrun.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.webrun.ai"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/enigmaclick",
      "github": "https://github.com/enigma"
    }
  },
  "redirects": [
    {
      "source": "/index",
      "destination": "/getting-started/overview"
    },
    {
      "source": "/quickstart",
      "destination": "/getting-started/quickstart"
    },
    {
      "source": "/sessions",
      "destination": "/concepts/sessions"
    },
    {
      "source": "/control",
      "destination": "/usage-guides/controlling-sessions"
    },
    {
      "source": "/events",
      "destination": "/api-reference/response-formats"
    },
    {
      "source": "/streaming",
      "destination": "/usage-guides/video-streaming"
    },
    {
      "source": "/errors",
      "destination": "/troubleshooting/errors-reference"
    },
    {
      "source": "/api-reference/introduction",
      "destination": "/api-reference/endpoints"
    }
  ]
}
